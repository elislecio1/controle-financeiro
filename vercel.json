{
  "version": 2,
  "functions": {
    "api/banco-inter.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/banco-inter",
      "destination": "/api/banco-inter.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/auth/callback",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
