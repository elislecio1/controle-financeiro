{
  "version": 2,
  "functions": {
    "api/banco-inter.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/banco-inter",
      "dest": "/api/banco-inter.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/auth/callback",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
